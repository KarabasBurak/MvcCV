@model MvcCV.Models.Entity.TblSertifikalarım
@{
    ViewBag.Title = "SertifikaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Sertifika Güncelleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Sertifika Açıklaması")
    @Html.TextBoxFor(x=>x.Aciklama, new { @class = "form-control" })
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-success">Güncelle</button>
    <a href="/Sertifika/SertifikaSil/@ViewBag.s" class="btn btn-outline-danger">Sil</a>
</form>

