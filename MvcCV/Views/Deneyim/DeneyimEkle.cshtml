
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<form method="post">
    <input type="text" name="Baslik" class="form-control" placeholder="Şirket"/>
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Pozisyon"/>
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="İşe Başlama-Bitiş Tarihi"/>
    <br />
    <textarea placeholder="Açıklama Giriniz" name="Aciklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

